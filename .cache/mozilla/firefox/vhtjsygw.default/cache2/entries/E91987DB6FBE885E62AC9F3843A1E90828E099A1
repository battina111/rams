<script type="text/javascript">
    
        $("#exit-button").click(function () {
        window.location = 'http://';
    });

    $("#exit-link").click(function () {
        window.location = 'http://';
        });
    
        if (bowser.chrome) {
        if (window.menubar && window.menubar.visible == false) {
            $('body').append('<div id="padsOverlay"></div>');
        }

        $(document).ready(function () {
            $('#padsOverlay').on('click', function () {
                window.open(window.location.href);
                setTimeout(function () {
                    window.close();
                }, 200);
            });
        });
    }

    if (bowser.firefox) {
        var img = document.createElement('img');

        img.addEventListener("load", function (e) {
            $('.btn, .legal').hide(); //hide download button
            $('#install-btn').parent().html('<h2 class="text-center">Already Installed!</h2>');
            $('.download-link').parent().append('<h2 class="text-center">Already Installed!</h2>');

        });

        img.setAttribute("src", "chrome://discretesearch/content/skin/images/ping.png");
    }
</script>


<script>
    $(document).ready(function () {
        $('.add-engine').on('click', function (e) {
            e.preventDefault();
            window.external.AddSearchProvider('https://www.discretesearch.com/update/installsearch?encToken=5d207df72e74dbd5099b1ca1f7f86b832a59d4ae79961979cd21c0c392fa536728a1807a3c85f6f74da02756189d41e3d58814f910ff0aac7f53b676bd50a584');
        });

        if (bowser.msie && !bowser.chrome) {
            $('#modal-close, .ext-modal-overlay').click(function (e) {
                e.preventDefault();
                $('.ext-modal-overlay').hide();
                $('.ext-modal').fadeOut('fast');
            });

            $("#install-btn > div > span.buttonText").html("Install Our Extension");
            $("#install-btn > span#add_to").html("IE");
            $("span.browserName").html("IE");
            $('#ext-badge').addClass('msie');
        }

        if (bowser.firefox) {
            $("#install-btn > div > span.buttonText").html('Add to Firefox');
            $("#install-btn > span#add_to").html("Firefox");
            $("span.browserName").html("Firefox");
        }

        if (bowser.chrome) {
            $("span.browserName").html("Chrome");
        }

        if (bowser.safari) {
            $("#install-btn > div > span.buttonText").html('Add to Safari');
            $("#install-btn > span#add_to").html("Safari");
            $("span.browserName").html("Safari");
        }

        $(".button-text").html($('#install-btn').text());
    });

    $(document).ready(function () {

        $('.chrome-overlay').hide();
        $('.firefox-overlay').hide();
        $('.safari-overlay').hide();
        $('#ie-wrap').hide();

        (function extensionInstaller() {
            var e = this;
            e.isChrome = bowser.chrome;
            e.isFirefox = bowser.firefox;
            e.isSafari = bowser.safari;

            e.showOverlay = function () {
                $('.chrome-overlay div.arrow-up').empty();
                $('.chrome-overlay div.arrow-up').append('<img src="/static/images/dl_click?encTk=YTE4NWJlZmEtNzczNS00ZWMwLTk3YmEtMzEzNjM5YzQ2NWYzCjIwMTcwNjA5CnNpLWV4dC1mZi1kcy1zZnQKc2Z0" />');
                $('.extension-overlay').fadeIn(500, function () {
                    $(".extension-overlay > div > div.arrow-up > img").effect('bounce', {
                        distance: 12,
                        direction: 'down',
                        times: 2
                    }, 2000);
                });

                if (e.isChrome) {
                    $('.chrome-overlay').show();
                } else if (e.isFirefox) {
                    $('.firefox-overlay').show();
                } else if (bowser.msie) {
                    $('#ie-wrap').show();
                } else if (e.isSafari) {
                    $('.safari-overlay').show();
                }
            };

            e.showTermsModal = function () {
                $("#acceptTermsModal").modal('show');
            };

            e.toggleModalContent = function (event) {
                if (event == "termsAccepted") {
                    $('.acceptTerms').hide();
                    $('.modal-footer').hide();
                    $('.termsAccepted').show();
                    e.fireInstall();
                }
            };

            e.hideOverlay = function () {
                $('.extension-overlay').fadeOut(200);
            };

                
                e.showCancelModal = function () {
                    $('.cancel-modal').fadeIn(500);
                };
                
                            
                e.hideCancelModal = function () {
                    $('.cancel-modal').fadeOut(500);
                }
                

            ////////////
            // EVENTS //
            ////////////
            e.hideModalEvent = (function () {
                $('.extension-overlay').on('click', e.hideOverlay);
            })();

            e.buttonClickEvent = (function () {
                $('#install-btn, .js-retry-install, .download-link').on('click', function (event) {
                    e.fireInstall();
                });

                    
                    $('.js-cancel-confirm').on('click', function () {
                        $('.cancel-overlay').hide();
                        e.cancelClicksEvents;

                    });

                    $('.js-retry-install').on('click', function () {
                        $('.cancel-overlay').hide();
                        $('.button').click();
                    });
                    
            })();

            e.fireInstall = function () {
                if (bowser.msie) {
                    $('.ext-modal-overlay').fadeIn(300);
                    $('.ext-modal').fadeIn(800);
                }
                else if (e.isFirefox) {
                    e.showOverlay();
                } else if (e.isChrome) {
                    
                }

                if (!bowser.msie && e.isChrome) {
                        
                            try{
                                chrome.webstore.install('https://chrome.google.com/webstore/detail/encibfeocgkgakjpdcpgjeclkcobmfpm'
                                                            , ChromeExtensionInstallSuccess
                                                            , ChromeExtensionInstallFail)
                            }
                            catch (err) {
                                ChromeExtensionInstallFail();
                            }
                        
                }
                if (e.isFirefox) {
                    var params = {
                        "Discrete Search": {
                            URL: "https://www.discretesearch.com/Content/branding/discretesearch/extensions/Redirect.xpi",
                            IconURL: "https://www.discretesearch.com/Content/branding/discretesearch/img/icon-large.png",
                            toString: function () { return "https://www.discretesearch.com/Content/branding/discretesearch/extensions/Redirect.xpi"; }
                        }
                    };
                    InstallTrigger.install(params);
                    checkFirefoxInstall();
                }
                if (e.isSafari) {
                    e.showOverlay();
                    safariInstallCallback();
                }
            };

                
            e.cancelClicksEvents = (function () {
                var cancelModal = $('.cancel-confirm');

                cancelModal.find('#yes').on('click', function (event) {
                    event.preventDefault();
                    e.hideCancelModal();
                    

                });

                cancelModal.find('#no').on('click', function (event) {
                    event.preventDefault();
                    e.hideCancelModal();
                    e.showOverlay();
                });
            })();
            

        })();
    });



    function safariInstallCallback() {
    
    }

    var ffLoop = { active: true, attempts: 0, maxAttempts: 1000 };

    function checkFirefoxInstall() {
        setTimeout(function () {
            var img = document.createElement('img');

            img.addEventListener("load", function (e) {
                ffLoop.active = false; //stop looping
                $('.extension-overlay').hide(); //hide overlay
                $('.btn').hide(); //hide download button
                $('#install-btn, .download-link').parent().html('<h2 class="text-center text-success">Thank you for installing!</h2>');

                    
                fireConversion("firefox");
                


            });

            img.addEventListener("error", function (e) {
                ffLoop.attempts++;
                if (ffLoop.attempts != ffLoop.maxAttempts)
                    checkFirefoxInstall();
            });

            img.setAttribute("src", "chrome://discretesearch/content/skin/images/ping.png");

        }, 200);
    }

    function ChromeExtensionInstallSuccess() {

        $('.extension-overlay').hide(); //hide overlay
        $('.btn').hide(); //hide download button
        $('#install-btn').parent().html('<h2 class="text-center text-success">Thank you for installing!</h2>');


            fireConversion("chrome");

    };

    function ChromeExtensionInstallFail() {
    };


    function fireConversion(browser) {

            appendFrame("https://f7boi.voluumtrk2.com/postback?cid=wDMIPE5V99ORNHV5H3MVV87J&txid=a185befa-7735-4ec0-97ba-313639c465f3");

    }

    function appendImgPixel(src) {
        var img = document.createElement('img');
        img.src = src;
        document.body.appendChild(img);
    }

    function appendFrame(src) {
        var iframe = document.createElement('iframe');
        iframe.src = src;
        iframe.scrolling = "no";
        iframe.frameborder = "0";
        iframe.width = 1;
        iframe.height = 1;

        document.body.appendChild(iframe);
    }
</script>

    
    <script>
        $(document).ready(function () {
            // Button text scrolling
            var button = $('.btn_text');
            setInterval(function () {
                if (!button.hasClass('alt-text')) {
                    button.html('<img src="/assets/img/sftc/downloadgif_right.gif"> FREE DOWNLOAD');
                    button.addClass('alt-text');
                } else {
                    button.html('<img src="/assets/img/sftc/downloadgif_right.gif"> CLICK TO INSTALL');
                    button.removeClass('alt-text');
                }
            }, 1800);
        });
    </script>

